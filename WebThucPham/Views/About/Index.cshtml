
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Begin Main Content Area -->
<main class="main-content">
    <div class="breadcrumb-area breadcrumb-height" data-bg-image="assets/images/breadcrumb/bg/1-1-1920x373.jpg">
        <div class="container h-100">
            <div class="row h-100">
                <div class="col-lg-12">
                    <div class="breadcrumb-item">
                        <h2 class="breadcrumb-heading">Giới Thiệu</h2>
                        <ul>
                            <li>
                                <a href="@Url.Action("Index","Home")">Trang chủ<i class="pe-7s-angle-right"></i></a>
                            </li>
                            <li>Giới thiệu về chúng tôi</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="about-banner different-bg-position section-space-y-axis-100" data-bg-image="/assets/images/about/banner/1-1-1920x598.jpg">
        <div class="container">
            <div class="row">
                <div class="col-xl-6 col-lg-5">
                    <div class="parallax-img-wrap">
                        <div class="papaya">
                            <div class="scene fill">
                                <div class="expand-width" data-depth="0.2">
                                    <img src="/assets/images/banner/inner-img/2-1-503x430.png" alt="Banner Images">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-7 align-self-center">
                    <div class="about-banner-content">
                        <div class="section-title">
                            <span class="sub-title">Xin Chào Đến Với Green Food</span>
                            <h2 class="title font-size-60 mb-6">Cửa Hàng Thực Phẩm Sạch</h2>
                            <p class="short-desc mb-0">Cửa hàng thực phẩm sạch Green Food luôn cung cấp đến khách hàng nguồn thực phẩm sạch rõ ràng về nguồn gốc, xuất xứ, trung thực trong chất lượng sản phẩm và hướng đến sức khỏe cộng đồng.</p>
                            <div class="button-wrap pt-8">
                                <a class="btn btn-custom-size lg-size btn-primary btn-dark-hover rounded-0" href="@Url.Action("Index","Product")">Đi Đến</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="intro-area section-space-y-axis-100">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-6">
                    <div class="intro-item text-white" data-bg-color="#aec581">
